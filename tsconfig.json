{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./lib",
    "target": "es2015",
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true
  },
}
